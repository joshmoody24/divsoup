# Example Terraform variables - COPY TO terraform.tfvars and fill in your actual values

# AWS region
aws_region = "us-west-1"

# Bucket name for analysis data
bucket_name = "divsoup"

# Environment 
environment = "prod"

# Database password (used for Aurora Serverless v2)
db_password = "REPLACE_WITH_SECURE_PASSWORD"

# SSH key for EC2 access (must be created in AWS first)
ssh_key_name = "your-key-name"

domain_name = "example.com"

letsencrypt_email = "fake@example.com"
